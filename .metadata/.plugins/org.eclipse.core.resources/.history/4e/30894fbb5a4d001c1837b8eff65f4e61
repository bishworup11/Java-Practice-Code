package threadDemo1;
class hi extends Thread
{
	@Override
	public void run() {
		for (int i = 0; i <5; i++) {
			System.out.println("hi");
			
		}
	}
	
}

class hello extends Thread
{
	@Override
	public void run() {
		for (int i = 0; i <5; i++) {
			System.out.println("hello");
			
		}
	}
	
}

public class ThreadDemo1 {

	public static void main(String[] args) {
		hi obj1=new hi();
		hello obj2= new hello();
		obj1.start();
		obj2.start();
		//obj1.run();
	

	}

}
